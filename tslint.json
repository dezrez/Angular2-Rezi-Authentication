{
  "rules": {
    "class-name": true,
    "curly": true,
    "eofline": true,
    "indent": [true, "spaces"],
    "max-line-length": false,
    "member-ordering": [
			true,
			{
				"order": [
					"statics-first"
				]
			}
    ],
    "no-arg": true,
    "no-construct": true,
    "no-duplicate-variable": true,
    "no-empty": true,
    "no-eval": true,
    "no-trailing-whitespace": false,
    "no-unused-expression": true,
    "no-unused-variable": true,
    "no-use-before-declare": true,
    "one-line": [
      true,
      "check-open-brace",
      "check-catch",
      "check-else",
      "check-whitespace"
    ],
    "quotemark": false,
    "semicolon": true,
    "triple-equals": [
      true,
      "allow-null-check"
    ],
    "variable-name": false,
    "whitespace": [
      true,
      "check-decl",
      "check-operator",
      "check-separator",
      "check-type"
    ]
  }
}
